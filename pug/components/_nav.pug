aside.sidebar
  h1.logo
    a.logo__link(href="#")
      span.logo__icon
  button.sidebar__toggle-button
    span Toggle
  .sidebar__collapse
    form.search(action="#")
      input.search__input(
        type="text"
        placeholder="Search"
        )
      button.search__btn(type="submit")
        span.search__btn-icon
    nav.navbar
      .dashboards
        -
          dashboardList = [
            {
              name  : "Overview",
              url : "#",
            },
            {
              name  : "Order history",
              url : "#",
            },
            {
              name  : "Fluid layout",
              url : "#",
            },
            {
              name  : "Icon nav",
              url : "#",
            }
          ]
        h3.dashboards__title dashboard
        ul.dashboards__list
          each val in dashboardList
            li.dashboards__item
              a.dashboards__link(href= val.url)= val.name
      .more-document
        -
          moreDocumentList = [
            {
              name  : "Overview",
              url : "#",
            },
            {
              name  : "Order history",
              url : "#",
            },
            {
              name  : "Fluid layout",
              url : "#",
            },
            {
              name  : "Icon nav",
              url : "#",
            }
          ]
        h3.more-document__title More
        ul.more-document__list
          each val in moreDocumentList
            li.dashboards__item
              a.dashboards__link(href= val.url)= val.name
